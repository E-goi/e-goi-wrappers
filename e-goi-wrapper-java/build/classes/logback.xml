<?xml version="1.0" encoding="UTF-8"?>
<configuration scan="true" scanPeriod="2000">

	<appender name="stdout" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%-5p %c{1}> %m%n</pattern>
		</encoder>
	</appender>
	
	<logger name="com.egoi.api.wrapper" level="DEBUG" />
	<logger name="org.apache.axis" level="DEBUG" />

	<root level="WARN">
		<appender-ref ref="stdout" />
	</root>
	
</configuration>